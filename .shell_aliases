# good experience with git.
alias g='git'
alias gs='git status'
alias gg='git status'
alias gc='git commit'
alias gca='git commit -a'
alias ga='git add'
alias go='git checkout'
alias gb='git branch'
alias gm='git merge'
alias gd="git diff"

# good experience with ls.
alias la='ls -al'

# good experience with ls.
alias ll='ls --color=auto -al'

# fast open the current folder in windows.
# TODO must find a way to test if working in MAC or Windows.
alias oo='explorer .'
